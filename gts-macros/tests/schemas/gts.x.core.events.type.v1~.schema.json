{
  "$id": "gts://gts.x.core.events.type.v1~",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "description": "Base event type with invalid ID field but valid GTS Type field",
  "properties": {
    "description": {
      "type": [
        "string",
        "null"
      ]
    },
    "gts_type": {
      "description": "GTS schema identifier",
      "format": "gts-schema-id",
      "title": "GTS Schema ID",
      "type": "string",
      "x-gts-ref": "gts.*"
    },
    "id": {
      "format": "uuid",
      "type": "string"
    },
    "name": {
      "type": "string"
    }
  },
  "required": [
    "gts_type",
    "id",
    "name"
  ],
  "title": "BaseEventV1MixedV1",
  "type": "object"
}